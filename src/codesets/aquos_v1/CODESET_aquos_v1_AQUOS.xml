  <action string="codeset" name="enablepower"              type="string"  cmd="RSPW2   "   />
  <action name="enablepower with wait" >
      <action  type="alias" cmd="enablepower" />
      <action string="wait" time="1000" />
  </action>

  <action name="power_on" >
      <action  type="alias" cmd="enablepower" />
      <action string="codeset" name="power_on" type="string" cmd="POWR1   " />
  </action>

  <action name="power_off" >
      <action  type="alias" cmd="enablepower" />
      <action string="codeset" name="power_on" type="string" cmd="POWR0   " />
  </action>

  <action name="power_toggle" >
      <action  type="alias" cmd="enablepower" />
      <action string="codeset" name="power_on" type="string" cmd="RCKY12  " />
  </action>

