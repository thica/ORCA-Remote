  <action string="codeset" name='right' waitforresponse='0' cmd="R\r\n" />
  <action string="codeset" name='up' waitforresponse='0' cmd="U\r\n" />
  <action string="codeset" name='down' waitforresponse='0' cmd="D\r\n" />
  <action string="codeset" name='exit' waitforresponse='0' cmd="o\r\n"  />
  <action string="codeset" name='esc' waitforresponse='0' cmd="o\r\n"  />

  <action string="codeset" name='home' waitforresponse='0' cmd="O\r\n" />
  <action string="codeset" name='reboot' waitforresponse='0' cmd="X\r\n" />
  <action string="codeset" name="power_on" waitforresponse='0' cmd="X\r\n" />
  <action string="codeset" name='suspend' waitforresponse='0' cmd='X\r\n' />
