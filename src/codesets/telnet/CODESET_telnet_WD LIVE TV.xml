<?xml version="1.0" encoding="UTF-8"?>
<!-- Translates Orca Actions to Interface Commands action = Orca Action String
     cmd = command to send, -->

<includes  xmlns:xi="http://www.w3.org/2001/XInclude">

  <repositorymanager>
    <entry>
      <name>WDTV Live control using Telnet</name>
      <description language='English'>WDTV Live Control using Telnet (incomplete)</description>
      <description language='German'>WDTV Live Control Ã¼ber Telnet (nicht komlett)</description>
      <author>Carsten Thielepape</author>
      <version>5.0.4</version>
      <minorcaversion>5.0.4</minorcaversion>
      <skip>1</skip>
      <sources>
        <source>
          <local>$var(APPLICATIONPATH)/codesets/telnet/CODESET_telnet_WD LIVE TV.xml</local>
          <sourcefile>$var(REPOSITORYWWWPATH)/codesets/CODESET_telnet_WD LIVE TV.zip</sourcefile>
          <targetpath>codesets/telnet</targetpath>
        </source>
      </sources>
      <dependencies>
        <dependency>
          <type>interfaces</type>
          <name>Telnet</name>
        </dependency>
      </dependencies>
    </entry>
  </repositorymanager>

  <action string="codeset" name='play' waitforresponse='0' cmd="&#32;\r\n" />
  <action string="codeset" name='pause' waitforresponse='0' cmd="&#32;\r\n" />

  <action string="codeset" name='ok' waitforresponse='0' cmd="&#10;\r\n" />
