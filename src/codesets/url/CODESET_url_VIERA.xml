  <action name='noise_reduction' string="codeset"   params='$var(vieraparamskeypre)NRC_P_NR-ONOFF$var(vieraparamskeypost)'        type="soap" waitforresponse='1' requesttype='POST' cmd='/nrc/control_0' headers="$var(vieraheaderkey)"  />
