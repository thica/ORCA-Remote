<?xml version="1.0" encoding="UTF-8"?>
<page name='Page_Device_$dvar(definition_alias_elv_max)'>
  <page_parameter>
    <interface>$var($dvar(definition_alias_elv_max)_INTERFACE_MAIN)</interface>
    <configname>$var($dvar(definition_alias_elv_max)_CONFIGNAME_MAIN)</configname>
  </page_parameter>
  <elements xmlns:xi="http://www.w3.org/2001/XInclude">
    <element name='Background' type='BACKGROUND' picturenormal='background' />
    <element name='Anchor BottomGap' type='ANCHOR' posx='left' posy='bottom' width='%100'  height='%1' />
    <element name='Anchor Info' type='ANCHOR' posx='center' posy='of:totop:Anchor BottomGap' width='%90' height='of:width:self:*0.9'>
      <xi:include href="$var(DEFINITIONPATHSKINELEMENTS)/block_buttons_info.xml" parse="xml" />
    </element>
    <element name='Anchor DropDowns' type='ANCHOR' posx='center' posy='of:totop:Anchor Info' width='of:width:Anchor Info'  height='%15' >
      <xi:include href="$var(DEFINITIONPATHSKINELEMENTS)/block_buttons_dropdowns.xml" parse="xml" />
    </element>
    <element name='Anchor Temp Knob' type='ANCHOR' posx='of:left:Anchor Info' posy='%10' width='%45' height='of:width:self:*1'>
      <xi:include href="$var(DEFINITIONPATHSKINELEMENTS)/block_knob_tempselect.xml" parse="xml" />
    </element>
    <element name='Picture Device' type='PICTURE' posx='%95' posy='%13' width='%40' height='of:width:self:*0.5' picturenormal='$var($dvar(definition_alias_elv_max)_DEVICEPICTURE)' />


  </elements>
</page>
