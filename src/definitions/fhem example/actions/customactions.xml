  <action name="$dvar(definition_alias_FHEMFS20LIGHTS)_Fkt Send Dim Dimmer">
    <action name="Set Bulb Transparancy" string="setwidgetattribute" widgetname="$var(DIMMERCOLORBALL)" attributename="transparancy" attributevalue="$var(DIMVALUE)"/>
    <action name="Switch Off on low Dim Values" string="goto label_switchoff" condition="$var(DIMVALUE)&lt;=3"  />
    <action name="Make FS Values" string="call Fkt DezToFS20DimSteps" />
    <action name="Dim it" string="sendcommand" commandname="dim" commandparameter='{"dimmer":"$var(DIMMER)","dimvalue":"$var(DIMVALUE)"}' interface="$var($dvar(definition_alias_FHEMFS20LIGHTS)_INTERFACE_1)" configname="$var($dvar(definition_alias_FHEMFS20LIGHTS)_CONFIGNAME_1)" />
    <action name="Done" string="goto label_end"/>
    <action name="label_switchoff" />
    <action name="Switch it off" string="sendcommand" commandname="power_off" commandparameter='{"dimmer":"$var(DIMMER)"}' interface="$var($dvar(definition_alias_FHEMFS20LIGHTS)_INTERFACE_1)" configname="$var($dvar(definition_alias_FHEMFS20LIGHTS)_CONFIGNAME_1)" />
    <action name="label_end" />
  </action>

