<?xml version="1.0" encoding="UTF-8"?>
<fontdef>

    <repositorymanager>
    <entry>
      <name>Default Fonts</name>
      <description language='English'>Orca default fonts</description>
      <description language='German'>Orca Standard Schriften</description>
      <author>Carsten Thielepape</author>
      <version>4.6.4</version>
      <minorcaversion>4.6.4</minorcaversion>
      <sources>
        <source>
          <local>$var(RESOURCEPATH)/fonts/Default</local>
          <sourcefile>$var(REPOSITORYWWWPATH)/fonts/default.zip</sourcefile>
          <targetpath>resources/fonts</targetpath>
        </source>
      </sources>
    </entry>
  </repositorymanager>

  <fonts>
    <font>
      <name>Sans</name>
      <file face="normal">$var(RESOURCEPATH)/fonts/Default/NotoSans-SemiCondensed.ttf</file>
      <file face="bold">$var(RESOURCEPATH)/fonts/Default/NotoSans-SemiCondensedBold.ttf</file>
      <file face="italic">$var(RESOURCEPATH)/fonts/Default/NotoSans-SemiCondensedItalic.ttf</file>
      <file face="bolditalic">$var(RESOURCEPATH)/fonts/Default/NotoSans-SemiCondensedBoldItalic.ttf</file>
    </font>
    <font>
      <name>Serif</name>
      <file face="normal">$var(RESOURCEPATH)/fonts/Default/NotoSerif-SemiCondensed.ttf</file>
      <file face="bold">$var(RESOURCEPATH)/fonts/Default/NotoSerif-SemiCondensedBold.ttf</file>
      <file face="italic">$var(RESOURCEPATH)/fonts/Default/NotoSerif-SemiCondensedItalic.ttf</file>
      <file face="bolditalic">$var(RESOURCEPATH)/fonts/Default/NotoSerif-SemiCondensedBoldItalic.ttf</file>
    </font>
    <font>
