<?xml version="1.0" encoding="utf-8"?>
<page name='Page_Export'>
    <page_parameter>
        <popup>1</popup>
        <preventpreinit>1</preventpreinit>
    </page_parameter>
    <elements>
        <element name='Background Dimmed' type='BACKGROUND' backgroundcolor='$var(dimmed)' />
        <element name="Button Back" type="BUTTON" posx="right" posy="top" width="%4" height="of:width:self" picturenormal="button wide*" action="gotosettingspage" fontsize='%40' caption="icon:close_window" />
        <element name='Title' type="TEXTFIELD" posx="center" posy="top" width="%90" height="of:height:Button Back"  fontsize='%h50' textcolor='$var(white)' caption='' htextalign='center' vtextalign='top' />
        <element name="Anchor" type="ANCHOR" posx="center" posy="middle" width="%90" height="%90"  picturenormal='background boxes' >
            <element name='Shadow' type='RECTANGLE' backgroundcolor='$var(shadow)' />
            <element name="FileBrowser" type="FILEBROWSER"  dirselect="1" action="Fkt Import_Export" actioncancel="gotosettingspage"/>
        </element>
    </elements>
</page>
